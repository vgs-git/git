
class Main {

  public static void main(String[] args) {
    
    int first = 10;
    int second = 20;

    int sum = first + second;
    System.out.println(first + " + " + second + " = "  + sum);
  }
}

public class IntToStringExample3{  
public static void main(String args[]){  
int i=200;  
String s=String.format("%d",i);  
System.out.println(s);  
}}  
